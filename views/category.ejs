<%- include ('layout/header') %>
    <%- include('layout/nav') %>
        <div class=container>
            <div class="mt-5">
                <h2>
                    Sacred and Religious Sites in Indonesia
                </h2>
            </div>
            <!-- search bar -->
            <div class="d-flex justify-content-center ms-auto me-auto col-lg-6 col-md-10 py-5">
                <div class="input-group">
                    <span class="input-group-text" id="basic-addon1">
                        <i class="bi bi-search"></i>
                    </span>
                    <input type="text" class="form-control" placeholder="Search" aria-describedby="basic-addon1">
                </div>
            </div>
        </div>
        <div>
            <div class="d-flex p-2 ">
                <!-- sidebar -->
                <div class="py-3 col-md-3 ms-0 d-none d-sm-block">
                    <h4>
                        Side Bar
                    </h4>
                </div>
                <!-- card -->
                <div class="col-lg-12">
                    <div class="card mb-3 border-0 mt-4 col-lg-12" style="max-width: 540px;">
                        <div class="row g-0">
                            <div class="col-md-6">
                                <div id="carouselExampleIndicators" class="carousel slide">
                                    <div class="carousel-indicators">
                                        <button type="button" data-bs-target="#carouselExampleIndicators"
                                            data-bs-slide-to="0" class="active" aria-current="true"
                                            aria-label="Slide 1"></button>
                                        <button type="button" data-bs-target="#carouselExampleIndicators"
                                            data-bs-slide-to="1" aria-label="Slide 2"></button>
                                        <button type="button" data-bs-target="#carouselExampleIndicators"
                                            data-bs-slide-to="2" aria-label="Slide 3"></button>
                                    </div>
                                    <div class="carousel-inner w-100 ">
                                        <div class="carousel-item active">
                                            <img src="https://cdn.discordapp.com/attachments/1108782723767746590/1109008829334687785/tanah-lot-temple-bali-island-indonesia.jpg"
                                            alt="Tanah Lot" class="img-fluid rounded d-block w-100 h-100 ">
                                        </div>
                                        <div class="carousel-item">
                                            <img src="https://cdn.discordapp.com/attachments/1108782723767746590/1109008829334687785/tanah-lot-temple-bali-island-indonesia.jpg"
                                            alt="Tanah Lot" class="img-fluid rounded d-block w-100 h-100">
                                        </div>
                                        <div class="carousel-item">
                                        <img src="https://cdn.discordapp.com/attachments/1108782723767746590/1109008829334687785/tanah-lot-temple-bali-island-indonesia.jpg"
                                            alt="Tanah Lot" class="img-fluid rounded d-block w-100 h-100">
                                        </div>
                                    </div>
                                    <button class="carousel-control-prev" type="button"
                                        data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
                                        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                                        <span class="visually-hidden">Previous</span>
                                    </button>
                                    <button class="carousel-control-next" type="button"
                                        data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
                                        <span class="carousel-control-next-icon" aria-hidden="true"></span>
                                        <span class="visually-hidden">Next</span>
                                    </button>
                                </div>
                            </div>
                            <div class="col-md-6 my-0">
                                <div class="card-body py-0 px-3">
                                    <h5 class="card-title">Tanah lot</h5>
                                    <p class="card-text">
                                        This is a wider card with supporting text below as a natural lead-in to
                                        additional content. This content is a little bit longer.
                                    </p>
                                    <p class="card-text">
                                        <small class="text-muted">Last updated 3 mins ago</small>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <%- include('layout/footer') %>